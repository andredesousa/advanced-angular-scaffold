{
  "name": "angular11",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "npm-run-all -p server \"ng serve -- --open --proxyConfig=proxies/server.config.js\"",
    "start:e2e": "npm-run-all -p server \"ng serve -- --prod --proxyConfig=proxies/server.config.js\"",
    "server": "node e2e/server/server.js",
    "models": "ng-openapi-gen --input openapi/users.yaml --output dist/@api/users",
    "check": "npm run lint && npm run test",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --run-in-band",
    "lint": "npm-run-all -s -c lint:ts lint:scss prettier lint:i18n lint:gherkin",
    "lint:fix": "npm-run-all -s -c lint:ts:fix lint:scss:fix prettier:fix lint:i18n:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:i18n": "ngx-translate-lint && jsonsort -c \"src/assets/i18n/*.json\"",
    "lint:i18n:fix": "jsonsort \"src/assets/i18n/*.json\"",
    "lint:gherkin": "gherkin-lint -c ./.gherkinlintrc.json",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --loglevel warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --loglevel warn",
    "storybook": "npm run docs -- -e json && start-storybook -p 6006",
    "storybook:build": "npm run docs -- -e json && build-storybook -o dist/storybook",
    "docs": "compodoc -p tsconfig.json src -d dist/documentation",
    "audit": "npm audit --production --json | npm run audit:html && npm run audit:check",
    "audit:html": "npm-audit-html --output dist/audit/index.html",
    "audit:check": "npm audit --production --audit-level critical",
    "sonar": "cross-env-shell SONAR_SCANNER_OPTS=-Djavax.net.ssl.trustStore=path-to-certificate/certificate.cert sonar-scanner",
    "clean": "rimraf dist/ .lighthouseci/",
    "clean:e2e": "rimraf dist/e2e/",
    "webdriver-manager:update": "webdriver-manager update --standalone false --gecko false",
    "e2e": "npm-run-all clean:e2e -s -c e2e:functional e2e:visual",
    "e2e:functional": "npm-run-all -p -r server functional",
    "e2e:visual": "npm-run-all -p -r server visual",
    "e2e:watch": "npm-run-all -p start:e2e protractor:watch",
    "functional": "ng e2e --suite=functional --webdriver-update=false",
    "visual": "ng e2e --suite=visual --webdriver-update=false",
    "protractor": "protractor e2e/protractor.conf.override.js",
    "protractor:watch": "wait-on http://localhost:4200/ && watch \"npm run protractor\" e2e/src --wait=1",
    "protractor:debug": "node --inspect-brk node_modules/protractor/bin/protractor e2e/protractor.conf.override.js",
    "analyze": "npm run build -- --stats-json --namedChunks=true && webpack-bundle-analyzer dist/angular11/stats.json",
    "build": "ng build --prod",
    "postbuild": "gzipper compress dist/angular11",
    "prerelease": "npm version major && git push",
    "release": "docker build -f cicd/docker/Dockerfile -t angular11 .",
    "deploy": "ansible-playbook cicd/deploy/deploy-to-swarm.yaml",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lighthouse": "lhci autorun --config=cicd/lighthouse/lighthouserc.json",
    "postci": "npm run postinstall",
    "postinstall": "npm-run-all -s webdriver-manager:update ivy models",
    "ivy": "ngcc",
    "ng": "ng"
  },
  "dependencies": {
    "@angular/animations": "11.1.1",
    "@angular/common": "11.1.1",
    "@angular/compiler": "11.1.1",
    "@angular/core": "11.1.1",
    "@angular/forms": "11.1.1",
    "@angular/platform-browser": "11.1.1",
    "@angular/platform-browser-dynamic": "11.1.1",
    "@angular/router": "11.1.1",
    "@ngrx/effects": "10.1.2",
    "@ngrx/store": "10.1.2",
    "@ngx-translate/core": "13.0.0",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@babel/core": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@lhci/cli": "^0.6.1",
    "@ng-apimock/core": "^2.4.0",
    "@ng-apimock/dev-interface": "^1.1.0",
    "@ng-apimock/protractor-plugin": "^1.1.0",
    "@ngrx/store-devtools": "^10.1.2",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/angular": "^6.1.16",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.3",
    "cucumber": "^6.0.5",
    "express": "^4.17.1",
    "gherkin-lint": "^4.1.3",
    "gzipper": "^4.4.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-marbles": "^2.5.1",
    "json-sort-cli": "^1.18.2",
    "lint-staged": "^10.5.2",
    "ng-mocks": "^11.5.0",
    "ng-openapi-gen": "^0.15.0",
    "ngx-translate-lint": "^1.14.1",
    "nodejs-sonar-scanner": "^4.0.4",
    "npm-audit-html": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "protractor-image-comparison": "^3.9.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.9.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-jasmine-rules": "^1.6.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1",
    "watch": "^1.0.2",
    "webdriver-manager": "^12.1.8",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
